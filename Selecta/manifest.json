{
  "name": "Selecta",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Javascript search for select html tag.",
  "homepage_url": "https://github.com/AlekseyMolchanov/Selecta",
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Selecta"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}